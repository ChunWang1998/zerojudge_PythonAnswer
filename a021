import math

astringinput,todo,bstringinput = input().split()
ainput = int(astringinput)
binput = int(bstringinput)
result = ""
if todo == '+':
    result = ainput+binput
elif todo == '-':
    result = ainput-binput
elif todo == '*':
    result = ainput*binput
elif todo == '/':
    result = int(ainput//binput)
print(result)
